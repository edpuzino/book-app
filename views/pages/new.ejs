<!DOCTYPE html>
<html lang="en">
  <%- include('../partials/head') %>
  <body>
  <%- include('../partials/header') %>
    <div id='page-label'>
      <h2 class='label'>Add A New Book</h2>
    </div>
    <form id='form' action='/books' method='post'>
        <input type="text" id="text" name='title' placeholder="Title">
        <input type="text" id="text" name='author' placeholder="Author">
        <input type="text" id="text" name='isbn' placeholder="ISBN">
        <input type="text" id="text" name='image_url' placeholder="Image URL">
        <textarea  id="text" name='description' placeholder="Description" cols="30" rows="5"></textarea>
        <a class='list' type='submit' id='button' href ="/books"> Submit</a>
    </form>
    <%- include('../partials/footer') %>
  </body>
</html>

