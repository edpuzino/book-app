<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head') %>
  <body>
    <%- include('./partials/header') %>
    <div id='page-label'>
      <h2 class='label'>Saved Books</h2>
    </div>
    <% books.forEach((book) => { %>
      <section class ="books">
        <ul class ='book'>
          <li class='items'>
          <img id = "book_img" src= "<%= book.image_url %>"/></li>
          <li class='items' id='title'><%= book.title%></li>
          <li class ='items' id='author'><%= book.author%></li>   
          <a class='list' id='details' href='books/:id'>Details!</a>
        </ul>
      </section>
    <% }); %>    
    <%- include('./partials/footer') %>
  </body>

</html>
